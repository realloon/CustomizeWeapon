<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- HeavyRounds -->
  <ThingDef ParentName="CWF_Module">
    <defName>CWF_Module_HeavyRounds</defName>
    <label>heavy rounds</label>
    <graphicData>
      <texPath>CustomizeWeapon/Modules/Ammo/Ammo</texPath>
    </graphicData>
    <statBases>
      <WorkToMake>1500</WorkToMake>
    </statBases>
    <costList>
      <Steel>10</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <modExtensions>
      <li Class="CWF.TraitModuleExtension">
        <weaponTraitDef>CWF_Trait_HeavyRounds</weaponTraitDef>
        <part>Ammo</part>
        <requiredWeaponTags>
          <li>CWF_AssaultRifle</li>
          <li>CWF_SniperRifle</li>
          <li>CWF_Handgun</li>
          <li>CWF_SMG</li>
          <li>CWF_LMG</li>
          <li>CWF_Minigun</li>
        </requiredWeaponTags>
      </li>
    </modExtensions>
  </ThingDef>

