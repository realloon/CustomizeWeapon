<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- VerticalGrip -->
  <ThingDef ParentName="CWF_TraitModule">
    <defName>CWF_Module_VerticalGrip</defName>
    <label>vertical grip</label>
    <graphicData>
      <texPath>CustomizeWeapon/Modules/Unberbarrel/Underbarrel</texPath>
    </graphicData>
    <statBases>
      <WorkToMake>1500</WorkToMake>
    </statBases>
    <costList>
      <Steel>10</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <recipeMaker>
      <researchPrerequisite>Gunsmithing</researchPrerequisite>
    </recipeMaker>
    <modExtensions>
      <li Class="CWF.TraitModuleExtension">
        <weaponTraitDef>CWF_Trait_VerticalGrip</weaponTraitDef>
        <part>Underbarrel</part>
        <graphicCases>
          <li>
            <matcher>
              <weaponDefs>
                <li>CWF_Gun_AssaultRifle</li>
              </weaponDefs>
            </matcher>
            <graphicData>
              <texturePath>CustomizeWeapon/Modules/Unberbarrel/VerticalGrip/VerticalGrip</texturePath>
              <outlinePath>CustomizeWeapon/Modules/Unberbarrel/VerticalGrip/VerticalGrip_o</outlinePath>
            </graphicData>
          </li>
          <!-- <li>
            <matcher>
              <weaponDefs>
                <li>CWF_Gun_HeavySMG</li>
              </weaponDefs>
            </matcher>
            <graphicData>
              <texturePath>CustomizeWeapon/Modules/Unberbarrel/VerticalGrip/ForHeavySMG</texturePath>
              <outlinePath>CustomizeWeapon/Modules/Unberbarrel/VerticalGrip/ForHeavySMG_o</outlinePath>
            </graphicData>
          </li> -->
        </graphicCases>
      </li>
    </modExtensions>
  </ThingDef>

